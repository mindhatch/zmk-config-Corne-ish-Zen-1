compatible = "zmk,combos";

#define COMBO(NAME, BINDINGS, KEYPOS, LAYERS, MS) \
  combo_##NAME { \
    bindings = <BINDINGS>; \
    key-positions = <KEYPOS>; \
    layers = <LAYERS>; \
    timeout-ms = <MS>; \
  };

COMBO(q, &kp Q, 3 4, susl sush, 20)
COMBO(j, &kp J, 2 3, susl sush, 20)
COMBO(z, &kp Z, 2 4, susl sush, 20)
COMBO(lpar, &kp LPAR, 4 16, meow susl sush navi, 20)
COMBO(rpar, &kp RPAR, 7 19, meow susl sush navi, 20)
COMBO(lbrc, &kp LBRC, 3 15, meow susl sush navi, 20)
COMBO(rbrc, &kp RBRC, 8 20, meow susl sush navi, 20)
COMBO(lbkt, &kp LBKT, 5 17, meow susl sush navi, 20)
COMBO(rbkt, &kp RBKT, 6 18, meow susl sush navi, 20)
COMBO(eql, &kp EQUAL, 32 33, meow susl sush navi, 20)
COMBO(minus, &kp MINUS, 31 32, meow susl sush navi, 20)
COMBO(astr, &kp STAR, 31 33, meow susl sush navi, 20)
COMBO(open, &kp LC(O), 17 29, meow susl sush navi, 20)
COMBO(rename, &kp F2, 28 29, meow susl sush navi, 20)
COMBO(pgdn, &kp PG_DN, 14 26, navi, 20)
COMBO(pgup, &kp PG_DN, 16 28, navi, 20)
COMBO(caps, &kp CAPS, 4 5, meow susl sush, 20)
COMBO(menu, &kp K_APP, 25 26, meow susl sush navi, 20)
COMBO(enter, &kp ENTER, 26 27, meow susl sush navi, 20)
COMBO(esc, &kp ESC, 26 27 28, meow susl sush navi, 20)
COMBO(tab, &kp TAB, 27 28, meow susl sush navi, 20)
COMBO(copy, &kp LC(C), 14 15, navi, 20)
COMBO(paste, &kp LC(V), 15 16, navi, 20)
COMBO(cut, &kp LC(X), 14 16, navi, 20)
COMBO(all, &kp LC(A), 14 15 16, navi, 20)
COMBO(playpause, &kp LC(LA(LS(P))), 2 3 4, navi, 20)
COMBO(volup, &kp K_VOL_UP, 3 4, navi, 20)
COMBO(voldn, &kp K_VOL_DN, 2 3, navi, 20)
COMBO(next, &kp K_NEXT, 2 4, navi, 20)
COMBO(prev, &kp K_PREV, 1 4, navi, 20)
COMBO(togsusl, &to susl, 1 5, meow susl sush, 20)
