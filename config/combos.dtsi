compatible = "zmk,combos";

#define COMBO(NAME, BINDINGS, KEYPOS, LAYERS) \
  combo_##NAME { \
    timeout-ms = <20>; \
    bindings = <BINDINGS>; \
    key-positions = <KEYPOS>; \
    layers = <LAYERS>; \
  };

COMBO(j, &kp j, 2 3, miaou)
COMBO(q, &kp q, 9 10, miaou)
COMBO(z, &kp z, 3 4, miaou)
COMBO(lpar, &kp LPAR, 4 16, miaou navi)
COMBO(rpar, &kp RPAR, 7 19, miaou navi)
COMBO(lbrc, &kp LBRC, 3 15, miaou navi)
COMBO(rbrc, &kp RBRC, 8 20, miaou navi)
COMBO(lbkt, &kp LBKT, 5 17, miaou navi)
COMBO(rbkt, &kp RBKT, 6 18, miaou navi)
COMBO(eql, &kp EQUAL, 32 33, miaou navi)
COMBO(minus, &kp MINUS, 31 32, miaou navi)
COMBO(astr, &kp STAR, 31 33, miaou navi)
COMBO(open, &kp LC(O), 17 29, miaou navi)
COMBO(rename, &kp F2, 28 29, miaou navi)
COMBO(pgdn, &kp PG_DN, 14 26, navi)
COMBO(pgup, &kp PG_DN, 16 28, navi)
COMBO(caps, &kp CAPS, 4 5, miaou)
COMBO(menu, &kp K_APP, 25 26, miaou navi)
COMBO(enter, &kp ENTER, 26 27, miaou navi)
COMBO(esc, &kp ESC, 26 27 28, miaou navi)
COMBO(tab, &kp TAB, 27 28, miaou navi)
COMBO(copy, &kp LC(C), 14 15, navi)
COMBO(paste, &kp LC(V), 15 16, navi)
COMBO(cut, &kp LC(X), 14 16, navi)
COMBO(all, &kp LC(A), 14 15 16, navi)
COMBO(playpause, &kp LC(LA(LS(P))), 2 3 4, navi)
COMBO(volup, &kp K_VOL_UP, 3 4, navi)
COMBO(voldn, &kp K_VOL_DN, 2 3, navi)
COMBO(next, &kp K_NEXT, 2 4, navi)
COMBO(prev, &kp K_PREV, 1 4, navi)